<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AccountFragment"
        tools:ignore="HardcodedText,RtlSymmetry">


        <Button
            android:id="@+id/fragment_account_logBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/edit_text_bordered"
            android:paddingHorizontal="35dp"
            android:text="Log OUT"
            app:backgroundTint="#FF9700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.885"
            tools:ignore="TextContrastCheck" />

        <ImageView
            android:id="@+id/fragment_account_imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="App Logo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.127"
            app:srcCompat="@drawable/app_logo" />


        <ImageView
            android:id="@+id/avatar"
            android:layout_width="129dp"
            android:layout_height="132dp"
            android:contentDescription="App Logo"
            app:layout_constraintBottom_toBottomOf="@+id/fragment_account_imageView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.484"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fragment_account_imageView"
            app:layout_constraintVertical_bias="0.471"
            app:srcCompat="@drawable/avatar" />

        <TextView
            android:id="@+id/fragment_account_phoneNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/account_squared_textview"
            android:paddingHorizontal="12dp"
            android:text="+91 "
            android:textSize="19dp"
            app:backgroundTint="#FF9700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.997"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.531"
            tools:ignore="TextContrastCheck" />


        <TextView
            android:id="@+id/fragment_account_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/account_squared_textview"
            android:paddingHorizontal="12dp"
            android:text=""
            android:textSize="19dp"
            app:backgroundTint="#FF9700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.996"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.612"
            tools:ignore="TextContrastCheck" />


        <TextView
            android:id="@+id/fragment_account_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/account_squared_textview"
            android:paddingHorizontal="12dp"
            android:text="Bhandup"
            android:textSize="19dp"
            app:backgroundTint="#FF9700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.695"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/fragment_account_fullname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/account_squared_textview"
            android:paddingHorizontal="12dp"
            android:text=""
            android:textSize="19dp"
            app:backgroundTint="#FF9700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.444"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/temp_acc_frag_Tname"
            android:layout_width="44dp"
            android:layout_height="22dp"
            android:text="Name"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.448" />

        <TextView
            android:id="@+id/temp_acc_frag_Tcontact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contact"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/fragment_account_phoneNo"
            app:layout_constraintHorizontal_bias="0.054"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.524" />

        <TextView
            android:id="@+id/temp_acc_frag_Temail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.605" />

        <TextView
            android:id="@+id/temp_acc_frag_TLocation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Location"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/fragment_account_location"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.687" />

    </androidx.constraintlayout.widget.ConstraintLayout>